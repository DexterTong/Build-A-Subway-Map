{
  "extends": "airbnb",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-use-before-define": ["error", "nofunc"],
    "no-plusplus": ["off"],
    "no-unused-vars": ["off"],
    "max-len": ["error", {
      "code": 120,
      "ignoreUrls": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }]
  }
}

{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-use-before-define": ["error", "nofunc"],
    "no-plusplus": ["off"],
    "no-unused-vars": ["warn", {"vars": "local", "args": "none"}],
    "max-len": ["error", {
      "code": 120,
      "ignoreUrls": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }]
  }
}
